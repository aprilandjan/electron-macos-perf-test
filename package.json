{
  "name": "electron-macos-perf-test",
  "version": "1.0.0",
  "description": "A minimal Electron application that show perf on macos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:arm64": "electron-builder --target=mas --arm64",
    "build:x64": "electron-builder --target=mas --x64",
    "build-no-sign": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -m"
  },
  "build": {
    "appId": "com.test",
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "18.3.6",
    "electron-builder": "^23.3.3"
  },
  "dependencies": {
  }
}
